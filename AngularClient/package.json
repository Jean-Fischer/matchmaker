{
  "name": "match-maker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4044",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate-client": "openapi-generator-cli generate -g typescript-angular -i http://localhost:5154/swagger/v1/swagger.json -o generated-sources/openapi",
    "proto:generate:win": "for /f %G in ('dir /b ..\\WebApi\\proto\\*.proto') do grpc_tools_node_protoc --plugin=protoc-gen-ng=.\\node_modules\\.bin\\protoc-gen-ng.cmd --ng_out=.\\generated-sources\\grpc -I ..\\WebApi\\proto ..\\WebApi\\proto\\%G"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@apollo/client": "^3.0.0",
    "@easydata/core": "^1.4.10",
    "@easydata/crud": "^1.4.10",
    "@easydata/ui": "^1.4.10",
    "@microsoft/signalr": "^6.0.7",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "@ngx-grpc/common": "^3.0.1",
    "@ngx-grpc/core": "^3.0.1",
    "@ngx-grpc/grpc-web-client": "^3.0.1",
    "@ngx-grpc/well-known-types": "^3.0.1",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "apollo-angular": "3.0.1",
    "bootstrap": "^5.1.3",
    "google-protobuf": "^3.20.1",
    "graphql": "^16",
    "grpc-web": "^1.3.1",
    "ngx-toastr": "^14.3.0",
    "rxjs": "~7.5.0",
    "ts-protoc-gen": "^0.15.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@ngx-grpc/protoc-gen-ng": "^3.0.1",
    "@types/google-protobuf": "^3.15.6",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.2"
  }
}
